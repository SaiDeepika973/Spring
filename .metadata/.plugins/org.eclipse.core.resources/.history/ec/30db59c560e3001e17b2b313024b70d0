package com.sathya.springex;

public class Doctor {
	
	private int id;
	private String name;
	
	//generating constructor
	
	public Doctor(int id, String name) {
		super();
		this.id = id;
		this.name = name;
		
		//doctor info method to print the doctor details
		
	}
		public Doctor doctorInfo()
		{
			Doctor doctor = new Doctor(id,name);
			return doctor;
		}
	}
