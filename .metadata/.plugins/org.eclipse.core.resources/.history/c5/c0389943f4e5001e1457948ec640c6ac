package com.sathya.lombok3;

import com.sathya.lombok.Employee;

import lombok.Builder;
@Builder
public class Test {
	
	public static void main(String[] args) {

	Employee employee = Employee.builder()
								.empId(101)
								.empName("Ratan")
								.empSalary(79000)
								.build();
	
	System.out.println(employee);
	}
}
